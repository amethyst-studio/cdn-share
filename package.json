{
  "name": "static-content-server",
  "description": "Simple Content Distribution Server for Personal/Private Applications (Commercial is allowed, but you my find security lacking in certain aspects.)",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Samuel J Voeller <samuel.voeller@amethyst.live> (https://chat.amethyst.live/)",
  "contributors": [],
  "homepage": "https://github.com/amethyst-studio/static-content-network",
  "repository": {
    "url": "git@github.com/amethyst-studio/static-content-network.git"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">7.11.0",
    "yarn": "use-npm"
  },
  "engineStrict": true,
  "scripts": {
    "prepare": "husky install",
    "runEslint": "eslint .",
    "dev": "npm run runEslint && NODE_ENV=development pm2 start ecosystem.config.js",
    "start": "npm run runEslint && NODE_ENV=production pm2 start ecosystem.config.js",
    "upgrade": "npx npm-check-updates -u --target latest --peer && wget https://www.toptal.com/developers/gitignore/api/node,yarn,webstorm+all,visualstudiocode,visualstudio -O ./.gitignore && cat ./custom.gitignore >> .gitignore"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "husky": "^6.0.0"
  },
  "dependencies": {
    "@oauth-everything/passport-discord": "^1.0.2",
    "@types/chai": "^4.2.18",
    "@types/luxon": "^1.26.5",
    "@types/mime-types": "^2.1.0",
    "@types/mocha": "^8.2.2",
    "@types/morgan": "^1.9.2",
    "@types/ms": "^0.7.31",
    "@types/node": "^15.3.0",
    "@types/restify": "^8.5.1",
    "@types/restify-errors": "^4.3.3",
    "amethyst-hv": "^0.2.5",
    "bandwidth-throttle-stream": "^1.1.1",
    "cryptocipher": "^3.0.6",
    "dotenv": "^9.0.2",
    "k-value": "^1.2.4",
    "luxon": "^1.27.0",
    "mime-types": "^2.1.30",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-restify": "^1.0.1",
    "restify": "^8.5.1",
    "restify-errors": "^8.0.2",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "winston": "^3.3.3"
  },
  "private": true,
  "root": true
}
